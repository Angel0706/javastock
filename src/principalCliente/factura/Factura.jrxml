	<queryString>
		<![CDATA[SELECT "PRODUCTS_SELLS".*, "PRODUCTS".*, (SELECT SUM("PRODUCTS_SELLS"."TOTAL") FROM  "PRODUCTS_SELLS" WHERE "PRODUCTS_SELLS"."SERIAL" = $P{serial} ) AS sum_all
FROM "PRODUCTS_SELLS" 
JOIN "PRODUCTS" ON "PRODUCTS_SELLS"."PRODUCT_ID" = "PRODUCTS"."PRODUCT_ID"
WHERE "PRODUCTS_SELLS"."SERIAL" = $P{serial}
]]>
	</queryString>
